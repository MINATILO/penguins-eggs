{
  "name": "penguins-eggs",
  "version": "2.2.6",
  "author": "Piero Proietti",
  "mail": "piero.proietti@gmail.com",
  "description": "remaster your system and distribuite it by ISO or via PXE",
  "license": "MIT",
  "homepage": "http://pieroproietti.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/pieroproietti/penguins-eggs"
  },
  "keywords": [
    "remastering",
    "pxe-server",
    "pxe-boot",
    "iso",
    "backup",
    "systemback"
  ],
  "bin": {
    "eggs": "./built/index.js"
  },
  "main": "./src/index.ts",
  "scripts": {
    "lint": "eslint src",
    "compile": "tsc",
    "esnext-compile": "babel ./esnext/ --out-dir ./built",
    "ts-start": "ts-node -- ./src/index.ts",
    "esnext-start": "npx babel-node ./esnext/",
    "node-start": "nodejs ./built"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "commander": "^2.15.1",
    "date-fns": "^1.29.0",
    "drivelist": "^5.2.12",
    "inquirer": "^5.2.0",
    "io": "^0.1.10",
    "ip": "^1.1.5",
    "netmask": "^1.0.6",
    "network": "^0.4.1",
    "pjson": "^1.0.9",
    "shelljs": "^0.8.2",
    "xml2js": "^0.4.19"
  },
  "config": {
    "unsafe-perm": true
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "0.0.41",
    "@types/ip": "0.0.30",
    "@types/netmask": "^1.0.30",
    "@types/node": "^10.12.23",
    "@types/shelljs": "^0.7.9",
    "@types/source-map-support": "^0.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.16.0",
    "ts-loader": "^4.5.0",
    "ts-node": "^6.2.0",
    "typescript": "^2.9.2"
  }
}
