{
  "name": "penguins-eggs",
  "description": "Perri's Brewery edition: remaster your system and distribuite it",
  "version": "7.8.38",
  "author": "Piero Proietti @pieroproietti",
  "bin": {
    "eggs": "bin/run"
  },
  "homepage": "https://penguins-eggs.net/",
  "repository": "https://github.com/pieroproietti/penguins-eggs",
  "bugs": "https://github.com/pieroproietti/penguins-eggs/issues",
  "dependencies": {
    "@getvim/execute": "^1.0.0",
    "@oclif/command": "^1.8.0",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "diskusage": "^1.1.3",
    "drivelist": "^9.2.4",
    "figlet": "^1.5.0",
    "globby": "11.0.2",
    "inquirer": "^8.0.0",
    "js-yaml": "^4.0.0",
    "line-reader": "^0.4.0",
    "mustache": "^4.1.0",
    "network": "^0.5.0",
    "perrisbrewery": "0.0.8",
    "pjson": "^1.0.9",
    "shelljs": "^0.8.4",
    "tslib": "^ 2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.3",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/clear": "^0.1.1",
    "@types/drivelist": "^6.4.1",
    "@types/inquirer": "^7.3.1",
    "@types/figlet": "^1.2.0",
    "@types/js-yaml": "^4.0.0",
    "@types/mocha": "^8.2.1",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.34",
    "@types/shelljs": "^0.8.8",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.31",
    "typescript": "^4.2.3"
  },
  "warn-if-update-available": {
    "timeoutInDays": 7,
    "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest). use: sudo eggs update %>.",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">8.17.0"
  },
  "files": [
    "/assets",
    "/addons",
    "/assistant",
    "/bin",
    "/conf",
    "/lib",
    "/npm-shrinkwrap.json",
    "/manpages",
    "/oclif.manifest.json",
    "/scripts",
    "/templates"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "eggs",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "scripts": {
    "docs": "rm docs -rf && npx typedoc src --out docs",
    "build": "tsc -p .",
    "deb": "oclif-dev pack:deb",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "types": "lib/index.d.ts"
}
