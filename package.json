{
  "name": "penguins-eggs",
  "shortName": "eggs",
  "description": "Perri's Brewery edition: remaster your system and distribuite it",
  "version": "8.1.2",
  "author": "Piero Proietti @pieroproietti",
  "bin": {
    "eggs": "bin/run"
  },
  "homepage": "https://penguins-eggs.net/",
  "repository": "https://github.com/pieroproietti/penguins-eggs",
  "bugs": "https://github.com/pieroproietti/penguins-eggs/issues",
  "dependencies": {
    "@getvim/execute": "^1.0.0",
    "@oclif/command": "^1.8.0",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "axios": "^0.21.1",
    "cfonts": "^2.9.3",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "ink": "github:vadimdemedes/ink",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "inquirer": "^8.0.0",
    "js-yaml": "^4.0.0",
    "mustache": "^4.2.0",
    "npm": "^8.11.0",
    "pjson": "github:pieroproietti/pjson",
    "react": "^16.14.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "./vendors/dev-cli",
    "@oclif/test": "^1.2.8",
    "@types/axios": "^0.14.0",
    "@types/clear": "^0.1.1",
    "@types/ink-big-text": "^1.2.0",
    "@types/ink-gradient": "^2.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/js-yaml": "^4.0.1",
    "@types/mocha": "^8.2.2",
    "@types/mustache": "^4.1.1",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "eslint": "^7.26.0",
    "globby": "^10.0.2",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "perrisbrewery": "./vendors/perrisbrewery",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "warn-if-update-available": {
    "timeoutInDays": 7,
    "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest). use: sudo eggs update %>.",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">8.17.0"
  },
  "files": [
    "/assets",
    "/addons",
    "/assistant",
    "/bin",
    "/conf",
    "/lib",
    "/manpages",
    "/oclif.manifest.json",
    "/scripts",
    "/templates"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "eggs",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "scripts": {
    "docs": "rm docs -rf && npx typedoc src --out docs",
    "build": "tsc -p .",
    "deb": "oclif-dev pack:deb && npx pb -m",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm lib -rf && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint-and-fix": "eslint . --ext .ts --fix --config .eslintrc",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "types": "lib/index.d.ts"
}
